{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dist/your-angular-app-name/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/server.js"
    }
  ]
}
